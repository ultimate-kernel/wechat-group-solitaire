page{
    background-color: #f5f5f5;
    position: relative;
    width: 100%;
    height: 100%;
}
::-webkit-scrollbar {
  display: none;
}

.activity-slideview-item {
  background: #f5f5f5;
  font-size: 16px;
  margin-bottom: 5px;
  border-radius: 5px;
  padding: 5px 0 5px 0px;
  text-indent: 10px;
}

.activity-slideview-item:last-child{
  margin-bottom: 0;
}

.weui-slideview__btn{
  padding: 5px 0 5px 0px;
}
