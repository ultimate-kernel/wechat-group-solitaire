page {
    background-color: #fff;
    position: relative;
    width: 100%;
    height: 100%;
}

::-webkit-scrollbar {
  display: none;
}

.form-container {
	background-color: #f5f5f5;
	width: 100%;
	overflow-y: auto;
}

.form-item {
  display: flex;
  padding: 10px;
  align-items: center;
  justify-content: space-between;
  border-bottom:1px solid rgb(238, 232, 232);
}

.form-item .button{
  width: 100%;
}
.item-cells{
  width: 100%;
  border-radius: 5px;
}

.car-location-add{
  padding: 10px 40px;
}

.slideview-item {
  background: #eee;
  text-indent: 10px;
  border-bottom: 1px solid #ddd;
}
